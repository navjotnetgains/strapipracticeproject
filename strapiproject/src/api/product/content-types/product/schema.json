{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "bags",
        "cases",
        "magpro",
        "subcardcases"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "features": {
      "type": "enumeration",
      "enum": [
        "featured",
        "bestsellers",
        "original"
      ]
    },
    "description": {
      "type": "text"
    },
    "describedFeatures": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
